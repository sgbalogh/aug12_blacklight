<div class="navbar-right">
  <ul class="nav navbar-nav">
    <%= render_nav_actions do |config, action|%>
      <li><%= action %></li>
    <% end %>
  </ul>

  <% if has_user_authentication_provider? %>
    <ul class="nav navbar-nav">
      <% if current_user %>
      <li>
        <!--Logout link (Nothing)-->
      </li>
      <% unless current_user.to_s.blank? -%>
      <li>
        <%= link_to current_user, 'https://eshelf.library.nyu.edu/' %>
      </li>
      <% end %>
      <% else %>
      <li>
        <%= link_to t('blacklight.header_links.login'), '/users/auth/nyulibraries' %>
      </li>
      <% end %>
      </ul>
  <% end %>
</div>
